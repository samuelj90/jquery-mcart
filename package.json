{
  "name": "jquery-mcart",
  "version": "1.0.0",
  "description": "An opinionated jquery plugin to perform simple e-commerce operations",
  "main": "index.js",
  "scripts": {
    "clean:all": "npm cache clean && npm run clean:dist && rimraf node_modules",
    "clean:dist": "rimraf dist && rimraf lint-reports",
    "build:dev": "npm run clean:dist && webpack --env=dev --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "npm run clean:dist && npm run test && webpack --env=prod --progress --profile --colors --display-error-details --display-cached",
    "start": "webpack-dev-server --env=dev --progress --profile --colors",
    "test": "karma start --no-auto-watch --single-run",
    "test:watch": "karma start --auto-watch --no-single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelj90/jquery-mcart.git"
  },
  "keywords": [
    "e-commerce",
    "jquery",
    "typescript",
    "html"
  ],
  "author": "Samuel J Mathew <samuelj90@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/samuelj90/jquery-mcart/issues"
  },
  "homepage": "https://github.com/samuelj90/jquery-mcart#readme",
  "dependencies": {
    "jquery": "^3.5.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.46",
    "@types/jquery": "~2.0.41",
    "@types/node": "~7.0.18",
    "connect-api-mocker": "^1.3.6",
    "copy-webpack-plugin": "~4.0.1",
    "ejs": "^2.5.7",
    "rxjs": "^5.5.6",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "phantomjs-prebuilt": "^2.1.14",
    "rimraf": "~2.6.1",
    "source-map-loader": "~0.2.0",
    "ts-loader": "~2.0.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.2.0",
    "tslint-loader": "~3.5.3",
    "typescript": "~2.3.2",
    "webpack": "~2.5.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0"
  }
}
